class Coordinate (object):
    def __init__(self,full_name,location,role):
        self.x = full_name
        self.y = location
        self.z = role
    def Coordinate (self):
        info = "full name:"+self.x+"   location:"+self.y+"   role:"+self.z
        print(info)
        return info
Z = Coordinate("John Wilson","International_Campus","Professor")
Z.Coordinate()
